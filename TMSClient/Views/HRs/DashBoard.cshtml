
@{
    ViewData["Title"] = "DashBoard";
    Layout = "~/Views/Shared/_Hr.cshtml";
}

<h1>DashBoard</h1>


<a class="btn btn-primary" asp-area="" asp-controller="Trainees" asp-action="Create">Add Trainee</a>

<div class="btn-group" role="group" aria-label="Button group with nested dropdown">
  <button type="button" class="btn btn-success">Trainee</button>
  <div class="btn-group" role="group">
    <button id="btnGroupDrop2" type="button" class="btn btn-success dropdown-toggle" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false"></button>
    <div class="dropdown-menu" aria-labelledby="btnGroupDrop2" style="">
      <a class="dropdown-item" asp-area="" asp-controller="Trainees" asp-action="Index">View Trainees</a>
      <a class="dropdown-item" asp-area="" asp-controller="Trainees" asp-action="Create">Add Trainee</a>
    </div>
  </div>
</div>



<div class="btn-group" role="group" aria-label="Button group with nested dropdown">
  <button type="button" class="btn btn-success">Trainer Managers</button>
  <div class="btn-group" role="group">
    <button id="btnGroupDrop2" type="button" class="btn btn-success dropdown-toggle" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false"></button>
    <div class="dropdown-menu" aria-labelledby="btnGroupDrop2" style="">
      <a class="dropdown-item" asp-area="" asp-controller="TrainerManagers" asp-action="Index">View Trainer Manager</a>
      <a class="dropdown-item" asp-area="" asp-controller="TrainerManagers" asp-action="Create">Add Trainer Manager</a>
    </div>
  </div>
</div>


<div class="btn-group" role="group" aria-label="Button group with nested dropdown">
  <button type="button" class="btn btn-success">Trainer</button>
  <div class="btn-group" role="group">
    <button id="btnGroupDrop2" type="button" class="btn btn-success dropdown-toggle" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false"></button>
    <div class="dropdown-menu" aria-labelledby="btnGroupDrop2" style="">
      <a class="dropdown-item" asp-area="" asp-controller="Trainers" asp-action="Index">View Trainers</a>
      <a class="dropdown-item" asp-area="" asp-controller="Trainers" asp-action="Create">Add Trainer</a>
    </div>
  </div>
</div>